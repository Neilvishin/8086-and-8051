;AN 8086 PROGRAM FOR MANIPULATING THE STRING USING REP
MOV [01400H], 010H 
MOV [01401H], 002H
MOV [01402H], 041H
MOV [01403H], 083H
MOV [01404H], 058H
MOV [01405H], 072H
MOV [01406H], 039H
MOV [01407H], 046H
MOV [01408H], 053H
MOV [01409H], 0A4H
MOV [0140AH], 030H
MOV [0140BH], 001H 

MOV SI, 01400H
MOV DI,01500H               
MOV CL, 00CH
CLD 
REP MOVSB                 
HLT      
